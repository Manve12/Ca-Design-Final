@model StoreGraphRenderer.Models.ClusterSelection

@{
    ViewBag.Title = "SelectCluster";
}

@using (Html.BeginForm("RenderButtons", "Home"))
{
    @Html.HiddenFor(group => group.ClusterGroupSelected);
    @Html.DropDownListFor(name => name.ClusterNameSelected, new SelectList(ViewBag.ListOfClusterNames));
    <input type="submit" value="Select" />
}